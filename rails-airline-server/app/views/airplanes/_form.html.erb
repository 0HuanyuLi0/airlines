<%= form_with url:'/airplanes',class:'border-form', local:true do |f| %>



<%= f.text_field :name, required:true, placeholder:"name #" %>



<%= f.number_field :seating_row, required:true, placeholder:"rows #"%>



<%= f.number_field :seating_column, required:true, placeholder:"cols #"%>


<br>
<%= f.submit 'Create New Airplane'%>

<% end %>